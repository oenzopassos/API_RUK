generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id         String       @id @default(uuid())
  name       String
  email      String    @unique
  password   String
  telephones Telephone[]
  createdAt  DateTime  @default(now())

  @@map("users")
}

model Telephone {
  id        String    @id @default(uuid())
  number    String
  area_code String
  userId    String
  user      User   @relation(fields: [userId], references: [id])

  @@map("telephones")
}